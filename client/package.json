	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-react": "^7.18.3",
		"eslint-plugin-react-hooks": "^2.5.0",
		"prettier": "^1.19.1",
		"xo": "^0.27.2"
	},
	"xo": {
		"parser": "babel-eslint",
		"extends": [
			"xo/esnext",
			"xo/browser",
			"xo-react"
		],
		"prettier": true,
		"env": [
			"es2020",
			"node",
			"browser"
		],
		"rules": {
			"unicorn/filename-case": [
				"error",
				{
					"cases": {
						"kebabCase": true,
						"pascalCase": true
					}
				}
			],
			"camelcase": [
				"error",
				{
					"properties": "never"
				}
			],
			"react/boolean-prop-naming": "off"
		}
	}
}
